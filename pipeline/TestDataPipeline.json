							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AzureSqlTableTest",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DelimitedTextCSV",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "DataPipelineProcessingTest",
				"description": "DataPipelineProcessingTest",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "MSSQL2ADLS",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Date_pipeline_test"
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricksTestPipeline",
					"type": "LinkedServiceReference"
				}
			}
		],
		"annotations": []
	}
