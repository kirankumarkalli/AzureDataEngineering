{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzureDataFactoryPracticeKiran"
		},
		"AzureDataLakeStorageTest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageTest'"
		},
		"AzureDatabricksTest_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksTest'"
		},
		"AzureDatabricksTestPipeline_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksTestPipeline'"
		},
		"AzureDataLakeStorageTest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kirandataengineering.dfs.core.windows.net/"
		},
		"AzureSqlDatabaseTest_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "kirandatapipeline.database.windows.net"
		},
		"AzureSqlDatabaseTest_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "kirandataengineeringpractice"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzureDataFactoryPracticeKiran_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageTest_accountKey": {
						"value": "[parameters('AzureDataLakeStorageTest_accountKey')]"
					},
					"AzureDatabricksTest_accessToken": {
						"value": "[parameters('AzureDatabricksTest_accessToken')]"
					},
					"AzureDatabricksTestPipeline_accessToken": {
						"value": "[parameters('AzureDatabricksTestPipeline_accessToken')]"
					},
					"AzureDataLakeStorageTest_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageTest_properties_typeProperties_url')]"
					},
					"AzureSqlDatabaseTest_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseTest_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseTest_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseTest_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}